{
  "name": "uned-tfm-app",
  "workspaces": [
    "packages/apps/*"
  ],
  "private": true,
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "format": "prettier --write \"packages/**/*.ts\"",
    "format:fix": "pretty-quick --pattern \"packages/**/*.ts\" --staged",
    "lint:fix": "eslint --ignore-path .eslintignore \"./packages/**/*.ts\" --fix",
    "lint": "eslint --ignore-path .eslintignore \"./packages/**/*.ts\"",
    "prepare": "husky install",
    "test": "jest",
    "test:malware-webapp": "yarn workspace malware-webapp-workspace run test",
    "start:malware-webapp": "yarn workspace malware-webapp-workspace run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uned-tfm/uned-tfm-app.git"
  },
  "author": "Jes√∫s <jbintaned1@alumno.uned.es>",
  "dependencies": {
    "axios": "0.27.2",
    "envalid": "7.3.1",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.1",
    "jest-mock-extended": "2.0.4",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/eslint": "8",
    "@types/jest": "27.4.1",
    "@types/node": "^18.15.11",
    "@types/prettier": "2",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "buffer": "^6.0.3",
    "husky": "8.0.1",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.1",
    "typescript": "4.6.3"
  }
}
